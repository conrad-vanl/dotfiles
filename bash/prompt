# Prompt
parse_git_branch() {
  git branch 2> /dev/null | sed -e '/^[^*]/d' -e 's/* \(.*\)/\1/'
}
parse_rbenv_version() {
  rbenv_ruby_version=`rbenv version | sed -e 's/ .*//'`
  printf $rbenv_ruby_version
}
export PS1='\n\[\033[0;37m\]in\[\033[0m\] \[\033[0;32m\]\W \[\033[0;37m\]on\[\033[0m\] \[\033[0;34m\]$(parse_git_branch)\[\033[0m\] \n\[\033[0;37m\]âœ“\[\033[0m\] '
